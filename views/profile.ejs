<%- include('includes/head.ejs') %>
  <!-- Page Header -->
  <header class="masthead">
    <div class="overlay"></div>
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
          <div class="post-heading">
            <h1>My Profile</h1>
          </div>
        </div>
      </div>
    </div>
  </header>

  <!-- Post Content -->
  <article>
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
          <h3>Username : </h3> 
          <%= user.username %>
        </div>
        <div class="col-lg-8 col-md-10 mx-auto">
          <h3>Email : </h3> 
          <%= user.email %>
        </div>
      </div>
    </div>
  </article>

  <hr>
<%- include('includes/end.ejs') %>